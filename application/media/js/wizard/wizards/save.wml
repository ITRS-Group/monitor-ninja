
<wizard title="setup">
	<step title="saving configuration">
		<info class="wizard-scan-status">Saving configuration...</info>
		<div class="wizard-scan-progress-wrapper"><div class="wizard-scan-progress"></div></div>
		<routine>

			$('.nextbutton').hide();
			$('.lastbutton').hide();
			$('.abortbutton').hide();
			$('.continuebutton').hide();
			$('.closebutton').attr('disabled', 'disabled');

			Wizard.save();

		</routine>
	</step>
</wizard>