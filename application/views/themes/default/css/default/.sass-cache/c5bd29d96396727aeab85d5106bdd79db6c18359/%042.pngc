// All sprites should extend this class
// The x24-sprite mixin will do so for you.
@if $x24-inline {
  #{$x24-sprite-base-class} {
    background-image: inline-sprite($x24-sprites);
  } 
} @else {
  #{$x24-sprite-base-class} {
    background: $x24-sprites no-repeat;
  }
}
//sass functions to return the dimensions of a sprite image as units

  @function x24-sprite-width($name) {
    $file: sprite_file($x24-sprites, $name);
    @return image-width($file);
  }

  @function x24-sprite-height($name) {
    $file: sprite_file($x24-sprites, $name);
    @return image-height($file);
  }


// Use this to set the dimensions of an element
// based on the size of the original image.
@mixin x24-sprite-dimensions($name) {
  @include sprite-dimensions($x24-sprites, $name)
}

// Move the background position to display the sprite.
@mixin x24-sprite-position($name, $offset-x: 0, $offset-y: 0) {
  @include sprite-background-position($x24-sprites, $name, $offset-x, $offset-y)
}

// Extends the sprite base class and set the background position for the desired sprite.
// It will also apply the image dimensions if $dimensions is true.
@mixin x24-sprite($name, $dimensions: $x24-sprite-dimensions, $offset-x: 0, $offset-y: 0) {
  @extend #{$x24-sprite-base-class};
  @include sprite($x24-sprites, $name, $dimensions, $offset-x, $offset-y)
}

@mixin x24-sprites($sprite-names, $dimensions: $x24-sprite-dimensions, $prefix: sprite-map-name($x24-sprites), $offset-x: 0, $offset-y: 0) {
  @include sprites($x24-sprites, $sprite-names, $x24-sprite-base-class, $dimensions, $prefix, $offset-x, $offset-y)
}

// Generates a class for each sprited image.
@mixin all-x24-sprites($dimensions: $x24-sprite-dimensions, $prefix: sprite-map-name($x24-sprites), $offset-x: 0, $offset-y: 0) {
  @include x24-sprites(add_save_search info link news refresh save_search settings shield-critical shield-disabled shield-down shield-error shield-help shield-info shield-not-critical shield-not-disabled shield-not-down shield-not-ok shield-not-pending shield-not-unknown shield-not-up shield-ok shield-outages shield-pending shield-unknown shield-unreachable shield-up shield-warning, $dimensions, $prefix, $offset-x, $offset-y);
